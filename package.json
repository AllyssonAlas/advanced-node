  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^26.0.24",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^8.0.1",
    "jest": "^27.0.6",
    "jest-mock-extended": "^2.0.7",
    "lint-staged": "^13.0.3",
    "pg-mem": "1.9.17",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/main",
    "dev": "ts-node-dev --respawn --transpile-only -r dotenv/config --inspect=0.0.0.0:9229 src/main",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "lint": "yarn eslint . --ext ts",
    "lint:fix": "yarn lint -- --fix",
    "prepare": "yarn husky install",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:coverage": "yarn test -- --coverage",
    "test:fb-api": "yarn test -- -c ./jest.integration.config.js ./tests/external/facebook-api.test.ts",
    "test:s3": "yarn test -- -c ./jest.integration.config.js ./tests/external/aws-s3-file-storage.test.ts",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:watch": "yarn test -- --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.1261.0",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.3",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "0.2.29",
    "uuid": "^9.0.0"
  }
}
